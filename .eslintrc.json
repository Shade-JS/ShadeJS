{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["xo", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": 2,
		"capitalized-comments": [
			"error",
			"always",
			{
				"ignorePattern": "*"
			}
		]
	}
}
